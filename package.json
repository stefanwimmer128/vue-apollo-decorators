{
  "name": "vue-apollo-decorators",
  "author": "Stefan Wimmer <info@stefanwimmer128.eu>",
  "version": "1.2.1",
  "description": "Use decorators to create Vue Apollo smart queries",
  "main": "cjs/index.js",
  "module": "lib/index.js",
  "browser": "dist/vue-apollo-decorators.min.js",
  "types": "lib/index.d.ts",
  "license": "ISC",
  "keywords": [
    "vue",
    "apollo",
    "vue-apollo",
    "decorator",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stefanwimmer128/vue-apollo-decorators"
  },
  "scripts": {
    "build": "run-s build:clean build:compile build:test build:bundle",
    "build:clean": "run-p build:clean:*",
    "build:clean:lib": "rimraf lib",
    "build:clean:cjs": "rimraf cjs",
    "build:clean:dist": "rimraf dist",
    "build:compile": "run-s build:compile:*",
    "build:compile:lib": "tsc && babel lib -d lib --plugins lodash --no-comments -s",
    "build:compile:cjs": "babel lib -d cjs --plugins @babel/transform-modules-commonjs -s",
    "build:test": "mocha -r source-map-support/register \"cjs/**/*.test.js\"",
    "build:bundle": "run-p build:bundle:*",
    "build:bundle:development": "cross-env NODE_ENV=development rollup -c -o dist/vue-apollo-decorators.js",
    "build:bundle:production": "cross-env NODE_ENV=production rollup -c -o dist/vue-apollo-decorators.min.js -p terser",
    "docs": "run-s docs:*",
    "docs:clean": "rimraf docs",
    "docs:render": "typedoc --out docs src --exclude \"src/test/**,src/**/*.test.ts\" --includeVersion --hideGenerator",
    "dev": "nodemon -w src -e ts -x \"run-s build\""
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.152",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "apollo-link-http-common": "^0.2.16",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "fs-extra": "^9.0.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "lodash-es": "^4.17.15",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.7",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^5.3.0",
    "source-map-support": "^0.5.19",
    "terser": "^4.7.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3",
    "vue": "^2.6.11",
    "vue-apollo": "^3.0.3",
    "vue-class-component": "^7.2.3"
  },
  "peerDependencies": {
    "graphql": "^15",
    "vue": "^2",
    "vue-apollo": "^3",
    "vue-class-component": "^7"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
